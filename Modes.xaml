        <ControlTemplate x:Key="SliderTemplate" TargetType="Slider">
            <Grid>

                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Grid.Column="0"
                    VerticalAlignment="Center"
                    FontFamily="Monotype Corsiva"
                    FontSize="20"
                    Foreground="Black"
                    Text="{DynamicResource Larger}" Padding="0,0,5,0" />
                <Grid
                    Grid.Row="0"
                    Grid.Column="1"
                    Height="16"
                    VerticalAlignment="Center">
                    <Grid.Background>
                        <ImageBrush ImageSource="Slider/Slider.png" />
                    </Grid.Background>
                    <Track
                        Name="PART_Track"
                        Width="128"
                        Height="16">

                        <Track.Thumb>
                            <Thumb
                                Width="16"
                                Height="16"
                                Foreground="{x:Null}"
                                Grid.IsSharedSizeScope="False"
                                SnapsToDevicePixels="True">
                                <Thumb.Template>
                                    <ControlTemplate>
                                        <Image Source="Slider/SliderThumb.png" />
                                    </ControlTemplate>
                                </Thumb.Template>
                            </Thumb>
                        </Track.Thumb>

                    </Track>
                </Grid>
                <TextBlock
                    Grid.Column="2"
                    VerticalAlignment="Center"
                    FontFamily="Monotype Corsiva"
                    FontSize="20"
                    Foreground="Black"
                    Text="{DynamicResource Smaller}" Padding="5,0,0,0" />
            </Grid>
        </ControlTemplate>
        <ControlTemplate x:Key="RectTemplate" TargetType="Button">
            <Grid HorizontalAlignment="Center">

                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />

                </Grid.ColumnDefinitions>
                <TextBlock
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    FontFamily="Monotype Corsiva"
                    FontSize="20"
                    Foreground="Black"
                    Text="{TemplateBinding Content}" />
                <Border
                    Grid.Row="1"
                    HorizontalAlignment="Center"
                    BorderBrush="#948a63"
                    BorderThickness="2">
                    <Grid
                        Width="48"
                        Height="32"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Stretch"
                        Background="{TemplateBinding Background}" />


                </Border>
            </Grid>
        </ControlTemplate>

        <ControlTemplate x:Key="MenuTemplate" TargetType="ToggleButton">
            <Grid>
                <Image Name="iButton">
                    <Image.Style>
                        <Style TargetType="Image">
                            <Setter Property="Source" Value="Buttons/MenuNormal.png" />
                        </Style>

                    </Image.Style>
                </Image>
                <Label
                    Name="lButton"
                    VerticalAlignment="Stretch"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    Content="{TemplateBinding Content}"
                    FontFamily="Monotype Corsiva"
                    FontSize="20"
                    Foreground="#FFFDFDFD" />
            </Grid>
            <ControlTemplate.Triggers>
                <Trigger Property="IsChecked" Value="True">
                    <Setter TargetName="iButton" Property="Source" Value="Buttons/MenuActive.png" />
                    <Setter Property="Width" Value="192" />
                </Trigger>
                <Trigger Property="IsChecked" Value="False">
                    <Setter Property="Width" Value="183" />
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>
        <ControlTemplate x:Key="BigButtonTemplate" TargetType="Button">
            <Grid>
                <Image Name="iButton">
                    <Image.Style>
                        <Style TargetType="Image">
                            <Setter Property="Source" Value="Buttons/ButtonNormal.png" />
                        </Style>

                    </Image.Style>
                </Image>
                <Label
                    Name="lButton"
                    VerticalAlignment="Stretch"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    Content="{TemplateBinding Content}"
                    FontFamily="Monotype Corsiva"
                    FontSize="22"
                    Foreground="#FFFDFDFD" />
            </Grid>
            <ControlTemplate.Triggers>
                <Trigger Property="IsEnabled" Value="False">
                    <Setter TargetName="iButton" Property="Source" Value="Buttons/ButtonDisable.png" />
                </Trigger>

                <Trigger Property="IsMouseOver" Value="True">
                    <Setter TargetName="iButton" Property="Source" Value="Buttons/ButtonActive.png" />
                </Trigger>
                <Trigger Property="IsPressed" Value="True">
                    <Setter TargetName="iButton" Property="Source" Value="Buttons/ButtonClicked.png" />
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>
        <ControlTemplate x:Key="ButtonTemplate" TargetType="Button">
            <Grid>
                <Image Name="iButton">
                    <Image.Style>
                        <Style TargetType="Image">
                            <Setter Property="Source" Value="Buttons/SmallButtonNormal.png" />
                        </Style>

                    </Image.Style>
                </Image>
                <Label
                    Name="lButton"
                    VerticalAlignment="Stretch"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Center"
                    Content="{TemplateBinding Content}"
                    FontFamily="Monotype Corsiva"
                    FontSize="20"
                    Foreground="#FFFDFDFD">
                    <Label.Effect>
                        <DropShadowEffect
                            BlurRadius="0"
                            RenderingBias="Quality"
                            ShadowDepth="1" />
                    </Label.Effect>
                </Label>
            </Grid>
            <ControlTemplate.Triggers>
                <Trigger Property="IsEnabled" Value="False">
                    <Setter TargetName="iButton" Property="Source" Value="Buttons/SmallButtonDisable.png" />
                </Trigger>

                <Trigger Property="IsMouseOver" Value="True">
                    <Setter TargetName="iButton" Property="Source" Value="Buttons/SmallButtonActive.png" />
                </Trigger>
                <Trigger Property="IsPressed" Value="True">
                    <Setter TargetName="iButton" Property="Source" Value="Buttons/SmallButtonClicked.png" />
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>
        <ControlTemplate x:Key="ToogleTemplate" TargetType="ToggleButton">

            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Border Padding="10,2,10,2">
                    <Image
                        Name="iButton"
                        Grid.Column="0"
                        Height="32">
                        <Image.Style>
                            <Style TargetType="Image">
                                <Setter Property="Source" Value="Buttons/RoundNormal.png" />
                                <Style.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter Property="Source" Value="Buttons/RoundActive.png" />
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </Image.Style>

                    </Image>
                </Border>
                <Label
                    Grid.Column="1"
                    VerticalAlignment="Center"
                    VerticalContentAlignment="Center"
                    Content="{TemplateBinding Content}"
                    FontFamily="{TemplateBinding FontFamily}"
                    FontSize="20"
                    Foreground="Black" />
            </Grid>
            <ControlTemplate.Triggers>

                <Trigger Property="IsChecked" Value="True">
                    <Setter TargetName="iButton" Property="Source" Value="Buttons/RoundActive.png" />
                </Trigger>
                <Trigger Property="IsEnabled" Value="False">
                    <Setter TargetName="iButton" Property="Source" Value="Buttons/RoundDisabled.png" />
                </Trigger>

            </ControlTemplate.Triggers>
        </ControlTemplate>
    </Window.Resources>
