    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:MultiTools"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="mW"
    Title="{DynamicResource TitleMods}"
    Width="833.625"
    Height="589.238"
    Activated="Window_Activated"
    Closing="Window_Closing"
    Cursor="{Binding AoE}"
    ResizeMode="CanMinimize"
    WindowStartupLocation="CenterScreen"
    WindowStyle="None"
    mc:Ignorable="d" ShowInTaskbar="False" Background="#00000000" AllowsTransparency="True">
    <Window.Resources>

        <Style BasedOn="{StaticResource {x:Type ToolTip}}" TargetType="ToolTip">
            <Setter Property="Background" Value="#CC000000" />
            <Setter Property="Placement" Value="Left" />
            <Setter Property="Foreground" Value="WhiteSmoke" />
            <Setter Property="BorderThickness" Value="2" />
            <Setter Property="BorderBrush">
                <Setter.Value>
                    <RadialGradientBrush>
                        <GradientStop Offset="0" Color="#FF4F4F4F" />
                        <GradientStop Offset="1" Color="#FFB09D24" />
                    </RadialGradientBrush>
                </Setter.Value>
            </Setter>
        </Style>
        <Style BasedOn="{StaticResource {x:Type Button}}" TargetType="Button">
            <EventSetter Event="PreviewMouseLeftButtonDown" Handler="Button_PreviewMouseLeftButtonDown" />
        </Style>
        <Style BasedOn="{StaticResource {x:Type RadioButton}}" TargetType="RadioButton">
            <EventSetter Event="PreviewMouseLeftButtonDown" Handler="Button_PreviewMouseLeftButtonDown" />
        </Style>
        <Style BasedOn="{StaticResource {x:Type ToggleButton}}" TargetType="ToggleButton">
            <EventSetter Event="PreviewMouseLeftButtonDown" Handler="Button_PreviewMouseLeftButtonDown" />
        </Style>



        <ControlTemplate x:Key="ContextTemplate1" TargetType="ContextMenu">
            <ListBox
                x:Name="colorList"
                Width="485"
                Height="220"
                Background="#CC000000"
                BorderThickness="1"
                ItemsSource="{Binding Colors}"
                ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                SelectionChanged="colorList_SelectionChanged">
                <ListBox.BorderBrush>
                    <RadialGradientBrush>
                        <GradientStop Offset="0" Color="#FF4F4F4F" />
                        <GradientStop Offset="1" Color="#FFB09D24" />
                    </RadialGradientBrush>
                </ListBox.BorderBrush>
                <ListBox.ItemsPanel>
                    <ItemsPanelTemplate>
                        <WrapPanel IsItemsHost="True" Orientation="Horizontal" />
                    </ItemsPanelTemplate>
                </ListBox.ItemsPanel>
                <ListBox.ItemTemplate>
                    <DataTemplate>
                        <Rectangle
                            Width="20"
                            Height="20"
                            Fill="{Binding Name}"
                            OpacityMask="Black"
                            Stroke="#FF211E1E"
                            StrokeThickness="1"
                            ToolTip="{Binding Name}" />
                    </DataTemplate>
                </ListBox.ItemTemplate>
            </ListBox>
        </ControlTemplate>
